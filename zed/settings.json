{
	"ssh_connections": [
  {
   "host": "73",
   "args": [],
   "projects": [
    {
     "paths": [
      "/home/wp"
     ]
    }
   ]
  }
 ],
 "agent": {
  "default_profile": "ask",
  "default_model": {
   "provider": "MiniMax-M2.1",
   "model": "MiniMax-M2.1"
  },
  "model_parameters": []
 },
 "language_models": {
  "openai_compatible": {
   "MiniMax-M2.1": {
    "api_url": "http://172.31.2.45/v1",
    "available_models": [
     {
      "name": "MiniMax-M2.1",
      "max_tokens": 64000,
      "max_output_tokens": 32000,
      "max_completion_tokens": 200000,
      "capabilities": {
       "tools": true,
       "images": false,
       "parallel_tool_calls": false,
       "prompt_cache_key": false
      }
     }
    ]
   },
   "LM-Studio": {
    "api_url": "http://192.168.31.91:1234/v1",
    "available_models": [
     {
      "name": "qwen/qwen3-4b-2507",
      "max_tokens": 200000,
      "max_output_tokens": 32000,
      "max_completion_tokens": 200000,
      "capabilities": {
       "tools": true,
       "images": false,
       "parallel_tool_calls": false,
       "prompt_cache_key": false
      }
     }
    ]
   }
  }
 },
 "telemetry": {
  "diagnostics": false,
  "metrics": false
 },
 "restore_on_startup": "none",
 "tabs": {
		"file_icons": true
	},
	"title_bar": {
		"show_branch_icon": true
	},
	"diagnostics": {
		"inline": {
			"enabled": true
		}
	},
	"inlay_hints": {
		"show_parameter_hints": false,
  "show_value_hints": true,
  "show_type_hints": false,
		"show_background": true,
		"enabled": true
	},
	"vim_mode": true,
	"redact_private_values": true,
	"icon_theme": {
  "mode": "system",
  "light": "Material Icon Theme",
  "dark": "Catppuccin Frapp√©"
 },
	"features": {
		"edit_prediction_provider": "none"
	},
	"ui_font_size": 16,
	"buffer_font_size": 16.0,
	// The name of a font to use for rendering text in the editor
	"buffer_font_family": "Maple Mono NF CN",
	"theme": {
		"mode": "system",
		"light": "Fleet Light",
		"dark": "Fleet Dark"
	},
	// export https_proxy=http://127.0.0.1:7897 http_proxy=http://127.0.0.1:7897 all_proxy=socks5://127.0.0.1:7897
	// "proxy": "http://127.0.0.1:7897",
	"languages": {
		"JavaScript": {
		  "formatter": {
				 "language_server": {
					  "name": "biome"
				}
			},
			"code_actions_on_format": {
			  "source.fixAll.biome": true,
        "source.organizeImports.biome": true
			}
		},
		"TypeScript": {
		  "formatter": {
				"language_server": {
				  "name": "biome"
				}
			},
			"code_actions_on_format": {
		    "source.fixAll.biome": true,
        "source.organizeImports.biome": true
			}
		},
		"TSX": {
		  "formatter": {
				"language_server": {
			    "name": "biome"
				}
			},
			"code_actions_on_format": {
		    "source.fixAll.biome": true,
        "source.organizeImports.biome": true
			}
		},
		"JSON": { "formatter": { "language_server": { "name": "biome" } } },
		"JSONC": { "formatter": { "language_server": { "name": "biome" } } },
		"CSS": { "formatter": { "language_server": { "name": "biome" } } }
	},
	"lsp": {
		"biome": {
			"settings": {
				"require_config_file": true
			}
		}
	}
}
